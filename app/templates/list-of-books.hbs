
<div class="container">
    <div class="page-header">
        <h1>Список книг<small> с Ember.js</small></h1>
    </div>
    <div class="row">
        <div class="col-md-6">
            <ul class="list-group">
              {{#each books as |book|}}
                  <li class="list-group-item">{{book.title}} by
                      <em>{{book.author}}</em>
                      <span class="rating pull-right">{{book.rating}}</span>
                  </li>
              {{/each}}
            </ul>
        </div>
    </div>
</div>

{{outlet}}


